{
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": ["**/node_modules/**", "**/dist/**"],
  "theme": "default",
  "name": "SportSee Documentation",
  "includeVersion": true,
  "categorizeByGroup": true,
  "searchInComments": true,
  "hideGenerator": false,
  "validation": {
    "invalidLink": false,
    "notExported": false
  }
}
